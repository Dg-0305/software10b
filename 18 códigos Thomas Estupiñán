   int vCubo=7*7*7; // el volumen de un cubo es una multiplicacion por tres
   System.out.println("el volumen de un cubo es:"+vCubo);
   
   /// calcular el volumen de una esfera15
   
   double RvE=7*7*7; //el radio de una esfera se hace multiplicando uno de sus lados por 3
   double pi1=3.14;
   double suma5=RvE*pi1;
   double numeroF= 4/3; 
   double VolE= suma5 * numeroF ;
   System.out.println("el volumen de una esfera es:"+VolE);
           
