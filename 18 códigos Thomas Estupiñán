   ///calcular la propina de un restaurante y el valor de la cuenta18
   
   double Valcuenta=350000;
   double propina= 0.20;
   double Descuento= Valcuenta * propina;
double total= Valcuenta+Descuento
   
   
   System.out.println("El valor de la propina es:"+Descuento);

System.out.println("el valor de la cuenta sin la propina es:"+Valcuenta);
System.out.println("el total a pagar es:"+total);
