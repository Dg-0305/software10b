    double precioOriginal=200.000; //precio del producto
    double porcentajeDescuento=15.0; //descuento en %
    double descuento=(precioOriginal * porcentajeDescuento) /100;
    double precioFinal= precioOriginal - descuento;
    System.out.println("17. Calcular el descuento aplicado a una compra (precio y % de descuento). ");
    System.out.println(descuento);
    System.out.println(precioFinal);
    
    double cuentaTotal=150.000; //total de la cuenta en el restaurante
    double porcentajePropina=10.0; //propina en %
